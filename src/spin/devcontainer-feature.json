{
    "name": "Spin",
    "id": "spin",
    "version": "1.0.7",
    "description": "Installs Spin framework",
    "options": {
        "plugins": {
            "type": "string",
            "description": "Specify Spin plugins that should be installed by default",
            "proposals": [
                "kube",
                "aka"
            ],
            "default": "kube,aka"
        },
        "version": {
            "type": "string",
            "proposals": [
                "latest",
                "canary",
                "3.5.1"
            ],
            "default": "latest",
            "description": "Specify the desired version of Spin"
        }
    },
    "installsAfter": [
        "ghcr.io/devcontainers/features/common-utils"
    ]
}